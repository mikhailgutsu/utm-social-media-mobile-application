{
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@assets": ["src/assets"],
      "@atoms": ["src/components/atoms"],
      "@molecules": ["src/components/molecules"],
      "@organisms": ["src/components/organisms"],
      "@navigation/*": ["src/navigation/*"],
      "@screens": ["src/screens"]
    }
  },
  "include": ["src", "App.tsx"]
}
